    "/api/v1/products": {
      "get": {
        "tags": ["Product"],
        "summary": "Get Products",
        "operationId": "get_products",
        "parameters": [
          {
            "required": true,
            "schema": {
              "title": "Category",
              "type": "array",
              "items": { "type": "string", "format": "uuid" }
            },
            "name": "category",
            "in": "query"
          },
          {
            "required": false,
            "schema": {
              "title": "Page",
              "minimum": 1.0,
              "type": "integer",
              "default": 1
            },
            "name": "page",
            "in": "query"
          },
          {
            "required": false,
            "schema": {
              "title": "Page Size",
              "minimum": 1.0,
              "type": "integer",
              "default": 100
            },
            "name": "page_size",
            "in": "query"
          },
          {
            "required": false,
            "schema": {
              "title": "Sort By",
              "pattern": "^(a_z|z_a)$",
              "type": "string",
              "default": "a_z"
            },
            "name": "sort_by",
            "in": "query"
          },
          {
            "required": false,
            "schema": {
              "title": "Price",
              "minimum": 0.0,
              "type": "array",
              "items": { "minimum": 0.0, "type": "integer" },
              "default": [1, 1000000]
            },
            "name": "price",
            "in": "query"
          },
          {
            "required": false,
            "schema": {
              "title": "Condition",
              "pattern": "^(new|used)$",
              "type": "string",
              "default": "new"
            },
            "name": "condition",
            "in": "query"
          },
          {
            "required": false,
            "schema": { "title": "Product Name", "type": "string" },
            "name": "product_name",
            "in": "query"
          }
        ],
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {
                  "title": "Response Get Products Api V1 Products Get"
                }
              }
            }
          },
          "422": {
            "description": "Validation Error",
            "content": {
